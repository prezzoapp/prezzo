{
  "name": "Prezzo",
  "author": "ishmaelthedestroyer",
  "description": "The digital menu for restaurants.",
  "homepage": "https://prezzo.io",
  "bugs": "https://github.com/odotan/prezzo/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/odotan/prezzo"
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/Prezzo/main.jsbundle --dev=false --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src test",
    "coverage": "rimraf coverage && jest --coverage",
    "version": "support/version-ios.sh",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "react-native": {
    "os": "react-native-os"
  },
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.3.5",
    "buffer": "^5.1.0",
    "eslint": "^5.1.0",
    "event-emitter": "^0.3.4",
    "immutable": "^3.7.6",
    "lodash": "^4.11.0",
    "moment": "^2.12.0",
    "native-base": "^2.4.5",
    "patch-package": "^5.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0-beta.5",
    "react-native": "^0.49.3",
    "react-native-browser-polyfill": "^0.1.2",
    "react-native-fbsdk": "^0.7.0",
    "react-native-google-places-autocomplete": "^1.3.6",
    "react-native-image-picker": "^0.26.10",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-os": "^1.2.1",
    "react-native-permissions": "^1.1.1",
    "react-native-slider": "^0.11.0",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "1.5.11",
    "react-navigation-redux-helpers": "^1.1.2",
    "react-redux": "^5.0.6",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-loop-symbol-ponyfill": "^2.2.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "standard-http-error": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-jest": "^17.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^5.5.0",
    "flow-bin": "^0.72.0",
