    "jest": "^17.0.2",
    "plop": "^1.7.4",
    "prettier": "^1.12.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.0.0",
    "react-native-mock": "~0.2.5",
    "react-test-renderer": "~15.4.2",
    "remote-redux-devtools": "^0.5.7",
    "rimraf": "^2.5.2"
  }
}
